        <PackageProjectUrl>https://github.com/quilin/Jamq.Client</PackageProjectUrl>
    </PropertyGroup>

    <ItemGroup>
        <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
